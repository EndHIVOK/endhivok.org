#
# Copy and rename this file to .env in the config folder.
#

# A common use case is to supply database creds via the environment. Edit settings.php
# like so:
#
# $databases['default']['default'] = [
#   'driver' => getenv('DB_DRIVER'),
#   'database' => getenv('DB_NAME'),
#   'host' => getenv('DB_HOST'),
#   'namespace' => 'Drupal\\Core\\Database\\Driver\\' . getenv('DB_DRIVER'),
#   'username' => getenv('DB_USER'),
#   'password' => getenv('DB_PASS'),
#   'port' => getenv('DB_PORT'),
#   'prefix' => getenv('DB_PREFIX'),
#   'collation' => 'utf8mb4_general_ci',
# ];
#
# When DB_DRIVER is sqlite, DB_NAME is a path to the SQLite file.
#
# Uncomment and populate as needed.

DB_NAME="../assets/.ht.sqlite"
#DB_HOST=
#DB_PASS=
#DB_PORT=
#DB_USER=
DB_DRIVER="sqlite"
#DB_PREFIX=
#DB_COLLATION="'collation' => 'utf8mb4_general_ci'"

SITE_NAME="EHOK Project Example Site"
SITE_MAIL="site@example.com"
ADMIN_USER="root"
ADMIN_EMAIL="admin@example.com"
FILE_PUBLIC_PATH="sites/default/files"
#FILE_PUBLIC_BASE_URL=http://downloads.example.com/files
FILE_PRIVATE_PATH="../assets/private_files"
UPDATE_FREE_ACCESS=FALSE
#HASH_SALT=
CONFIG_ACTIVE_DIRECTORY="../config/active"
CONFIG_SYNC_DIRECTORY="../config/sync"



# Another common use case is to set Drush's --uri via environment.
